{
  "name": "save-n-org",
  "version": "0.0.0",
  "main": "./dist/electron/app.js",
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.9.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "electron": "ng build --base-href ./ && tsc --p src/electron && electron ./dist/electron/app.js",
    "electron:test": "jasmine-ts --config=src/electron/jasmine.json -P src/electron/tsconfig.json",
    "e2e": "ng e2e",
    "client:dev": "ng serve --proxy-config src/proxy.conf.json", 
    "server:dev": "npx nodemon --watch 'src/electron/**' --ext 'ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node -P src/electron/tsconfig.json src/electron/server.ts'",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.7",
    "@angular/cdk": "^12.2.8",
    "@angular/cli": "^12.2.7",
    "@angular/common": "~12.2.7",
    "@angular/compiler": "~12.2.7",
    "@angular/core": "12.2.7",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.7",
    "@angular/material": "^12.2.8",
    "@angular/platform-browser": "~12.2.7",
    "@angular/platform-browser-dynamic": "~12.2.7",
    "@angular/router": "~12.2.7",
    "angular-resizable-element": "^3.3.3",
    "electron-squirrel-startup": "^1.0.0",
    "ngx-quill": "^12.0.1",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/compiler-cli": "~12.2.7",
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.37",
    "body-parser": "^1.19.0",
    "codelyzer": "^5.1.2",
    "electron": "^9.2.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jasmine-ts": "^0.4.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^2.0.15",
    "protractor": "~7.0.0",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "save_and_organize"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
